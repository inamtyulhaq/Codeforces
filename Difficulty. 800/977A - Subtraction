#include <iostream>
using namespace std;

int main()
{
    int n, k;
    cin >> n >> k;
    for(int i = 1; i <= k; i++)
    {
        int rem = n % 10;
        if(rem != 0)
        {
            n = n - 1;
        }
        else if(rem == 0)
        {
            n = n / 10;
        }
    }

    cout << n;
    return 0;
}
